  "peerDependencies": {
    "react": ">=16.3.0",
    "react-redux": "^6.0.0",
    "redux": ">=4.0.0",
    "redux-saga": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
