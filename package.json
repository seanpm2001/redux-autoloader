    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "redux-saga": "1.0.0",
    "sinon": "1.17.7",
    "webpack": "4.29.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "@babel/runtime": "7.3.1",
    "eslint-plugin-jest": "22.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "react-display-name": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
